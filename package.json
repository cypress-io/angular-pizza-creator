    "@types/jasmine": "2.8.16",
    "@types/karma": "0.13.36",
    "@types/node": "7.10.6",
    "@ultimate/aot-loader": "0.1.16",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.5.0",
    "cypress": "3.4.1",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "jasmine-core": "2.5.2",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.28",
    "karma-webpack": "2.0.2",
